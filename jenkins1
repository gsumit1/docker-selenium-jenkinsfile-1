node {
        stage('Initialize'){
	 def dockerHome = tool 'dockerHome'
 	 def mavenHome  = tool 'MAVEN_HOME'
        env.PATH = "${dockerHome}/bin:${mavenHome}/bin:${env.PATH}"
    	}
        stage('Build Jar') {
                sh 'mvn clean package -DskipTests'
        }
        stage('Build Image') {
           	sh 'docker.build(\"sghosh1/dodocker\")'
        }
            
}
